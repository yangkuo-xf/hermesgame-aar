<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/qg_light_orange2"></solid>

            <corners android:radius="20dp" />
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/qg_light_orange2"></stroke>
            <solid android:color="@color/qg_light_orange2"></solid>
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/qg_btn_code_false"></stroke>
            <solid android:color="@color/qg_btn_code_false"></solid>
            <corners android:radius="20dp" />
        </shape>
    </item>


</selector>


